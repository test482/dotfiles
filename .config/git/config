[color]
    diff = auto
    status = auto
    branch = auto
    grep = auto
    showbranch = auto
[alias]
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cd) %C(bold blue)<%an>%Creset' --abbrev-commit
    st = status
    co = checkout
    cm = commit
    br = branch
    last = log -1 --stat
    undo = reset HEAD^
[log]
    date = iso
